	localizationClasses='netgest.bo.xwc.xeo.workplaces.admin.localization.MainAdminViewerMessages'>
		<xeo:formLookupList>
			<xvw:window id="lookupWindow" width="550" height="400">
				<!-- <xvw:title value="Lista de Regras"/>
				<xvw:gridPanel rowSelectionMode="#{viewBean.rowSelectionMode}" id="lookupPanel" dataSource="#{viewBean.dataList}" onRowDoubleClick="#{viewBean.rowDoubleClick}"> -->
				<xeo:list>
					<xvw:columns>
                		<!--<xvw:columnAttribute width='10'  label='&nbsp;' hideable='false' sortable='false' groupable='false' searchable='false' dataField='SYS_OBJECT_ICON_16'/> -->
