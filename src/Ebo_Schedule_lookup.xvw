						<xvw:columnAttribute width="150" dataField="description"/>
						<xvw:columnAttribute width="150" dataField="typePolicy"/>
					</xvw:columns>
					<xvw:actionButton label="@{CONFIRM}" serverAction="#{viewBean.confirm}"/>
				<!--</xvw:gridPanel> -->
				</xeo:list>
			</xvw:window>
		</xeo:formLookupList>
	</xvw:viewer>
