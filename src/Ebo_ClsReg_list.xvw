<xvw:root xmlns:xvw="http://www.netgest.net/xeo/xvw">
	<xvw:viewer beanClass="netgest.bo.xwc.components.beans.XEOBaseList" beanId="viewBean">
		<xvw:form>
			<xvw:title valueExpression="#{viewBean.title}"/>
			<xvw:gridPanel 
					rowSelectionMode="" 
					dataSource="#{viewBean.dataList}" 
					onRowDoubleClick="#{viewBean.rowDoubleClick}" 
					id="users" 
					autoExpandColumn="name"
				>
				<xvw:columns>
                	<xvw:columnAttribute width='10'  label='&nbsp;' hideable='false' sortable='false' groupable='false' searchable='false' dataField='SYS_OBJECT_ICON_16'/>
					<xvw:columnAttribute width="150" label="Nome" dataField="name"/>
					<xvw:columnAttribute width="200" label="Label" dataField="label"/>
					<xvw:columnAttribute width="300" label="Descrição" dataField="description"/>
					<xvw:columnAttribute width="150" label="Package" dataField="xeopackage"/>
				</xvw:columns>
			</xvw:gridPanel>
		</xvw:form>
	</xvw:viewer>
</xvw:root>
