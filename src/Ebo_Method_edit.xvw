<xvw:root xmlns:xvw="http://www.netgest.net/xeo/xvw"
xmlns:xeo="http://www.netgest.net/xeo/xeo">
	<xvw:viewer beanClass="netgest.bo.xwc.components.beans.XEOBaseOrphanEdit" beanId="viewBean" localizationClasses='netgest.bo.xwc.xeo.workplaces.admin.localization.MainAdminViewerMessages'>
		<xeo:formEdit>
			<!--  <xvw:window id="editWindow" width="550" height="400">-->
				<xvw:panel>
					<xvw:panel>
						<xvw:rows>
							<xvw:row>
								<xvw:cell>
									<xvw:attribute objectAttribute="name" enableCardIdLink="false" disabled="false"/>
								</xvw:cell>
							</xvw:row>
							<xvw:row>
								<xvw:cell>
									<xvw:attribute objectAttribute="label" disabled="false"/>
								</xvw:cell>
							</xvw:row>
							<xvw:row>
								<xvw:cell>
									<xvw:attribute objectAttribute="clsReg" disabled="false"/>
								</xvw:cell>
							</xvw:row>
						</xvw:rows>
					</xvw:panel>
					<xvw:tabs>
						<xvw:tab label="@{SECURITY}">
							<xvw:rows>
								<xvw:row>
									<xvw:gridPanel rowSelectionMode="MULTI_ROW" objectAttribute="security" id="security" onRowDoubleClick="disabled">
										<xvw:toolBar>
											<xvw:menu text="@{ADD}" icon="ext-xeo/images/menus/adicionar-bridge.gif" value="form1Method:security" serverAction="#{viewBean.lookupBridge}"/>
											<xvw:menu text="@{REMOVE}" icon="ext-xeo/images/menus/remover-bridge.gif" value="form1Method:security" serverAction="#{viewBean.removeFromBridge}"/>
										</xvw:toolBar>
										<xvw:columns>
											<xvw:columnAttribute width="300" label="@{FORM_NAME}" dataField="name"/>
											<xvw:columnAttribute width="200" label="@{EMAIL}" dataField="email"/>
										</xvw:columns>
									</xvw:gridPanel>
								</xvw:row>
							</xvw:rows>
						</xvw:tab>
					</xvw:tabs>
				</xvw:panel>
			<!-- </xvw:window> -->
		</xeo:formEdit>
	</xvw:viewer>
</xvw:root>
